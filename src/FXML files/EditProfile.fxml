            <Button layoutX="220.0" layoutY="325.0" mnemonicParsing="false" onAction="#resetAction" prefWidth="100.0" styleClass="GreenButton" stylesheets="@../CSS%20files/Button.css" text="Reset" textFill="WHITE">
