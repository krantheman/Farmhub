            <Label layoutX="100.0" layoutY="30.0" prefHeight="40.0" text="Change Password" textFill="WHITE">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
